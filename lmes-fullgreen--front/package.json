{
  "name": "fullgreen",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode local",
    "serve:dev": "vue-cli-service serve --mode development",
    "serve:test": "vue-cli-service serve --mode test",
    "serve:pre": "vue-cli-service serve --mode test",
    "serve:pro": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build --mode local",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --mode development",
    "build:test": "vue-cli-service build --mode test",
    "build:pre": "vue-cli-service build --mode test",
    "build:pro": "vue-cli-service build --mode production",
    "build:docker-local": "VUE_APP_CONFIG_FOR=docker vue-cli-service build --mode local",
    "build:docker-dev": "VUE_APP_CONFIG_FOR=docker vue-cli-service build --mode dev",
    "build:docker-test": "VUE_APP_CONFIG_FOR=docker vue-cli-service build --mode test",
    "build:docker-pre": "VUE_APP_CONFIG_FOR=docker vue-cli-service build --mode pre",
    "build:docker-pro": "VUE_APP_CONFIG_FOR=docker vue-cli-service build --mode pro",
    "build:docker": "VUE_APP_CONFIG_FOR=docker vue-cli-service build --mode pro"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap-vue": "^2.6.1",
    "http-server": "^0.12.1",
    "vue": "^2.6.10",
    "vue-http-server": "0.0.1",
    "vue-material": "^1.0.0-beta-11",
    "vue-router": "^3.1.3",
    "vuetify": "^2.2.18",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
